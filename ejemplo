;script con funciones dummy para practicar

; Definicion de Expresion Booleana á¹”robabilista
(declare-datatypes () ((ProbBool (pb (p Real) (q Real)))))
(define-fun falseVal ((c ProbBool)) ProbBool
  c )

(define-fun isEmpty ((x ProbBool)) ProbBool (
  match x (
    ((pb p q) (pb (+ 4 p q) q))
  )
))

(simplify (isEmpty (pb 1 1)))

